<script type="text/javascript">

//index page



	$(function() {

		var $menuExpansion = $("#container_right").find('.expander');

		//runs on document load -- allows non JS users to see menu
		//Commented out to prevent collapse of right side menu.
		//$(".expandMenu").hide();
		
		
	
   		$menuExpansion.click(function(event){
			event.preventDefault(event);
			if ($(this).siblings().hasClass("expanded")){ //If it's open
			$(this).siblings('.expandMenu').slideToggle().removeClass("expanded");//close the item
				}
			
			else {//it's not open
			$('.expanded:not("#box_right_inside,#box2_right_inside,#box3_right_inside")').slideToggle().removeClass("expanded");//close everything but the main boxes
			$('.box2_right_ul li').removeClass("live");
			$(this).next('.expandMenu').eq(0).addClass("expanded").slideToggle();//open up
			$(this).parent('li').addClass("live");//make the open item white
			}
   		});
		$("#box_right_inside").show().addClass("expanded");//opens Key statistics Right Bar on Index on load
		

	});
	
//topics landing
	$(function() {
		var $menuExpansion = $("#box3_content_inside").find('.expander');
		//runs on document load
		$("#box3_content_inside").find("menuExpansion:first-child .expandMenu").show();
	
   		$menuExpansion.click(function(event){
			event.preventDefault(event);
			if ($(this).next('.expandMenu').hasClass("expanded")){ //If the accordion is already open
			$(this).next('.expandMenu').eq(0).slideToggle().removeClass("expanded");//close the item
			$(this).find("img").attr("src","/images/1_plus_icon2.png");

				}
			else {//this accordion is not not open 
			$(this).next().addClass("expanded").slideToggle();//open up
			$(this).find("img").attr("src","/images/1_minus_icon2.png");
			
			
			}
   		});
		//topics Index page
		$(".menuExpansion .corrections_accord + .expandMenu").show().addClass("expanded");//opens corrections tab and first accordion of first item
	



	});
	
	//changes which tabs are open when arriving from a topic detail page
	
	$(function(){
		
	function GetUrlValue(VarSearch){ //get the parameter from url
    var SearchString = window.location.search.substring(1);
    var VariableArray = SearchString.split('&');
    for(var i = 0; i < VariableArray.length; i++){
        var KeyValuePair = VariableArray[i].split('=');
        if(KeyValuePair[0] == VarSearch){
            return KeyValuePair[1];
        }
    }
}	
	var tidVal = GetUrlValue('tid');
		
		if (tidVal=="2") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
	$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
	$(".menuExpansion .courts_accord + .expandMenu").show().addClass("expanded");
	$(".menuExpansion .courts_accord").attr("src","/images/1_minus_icon2.png");
	}
else if (tidVal=="3") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
		$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
$(".menuExpansion .crime-type_accord + .expandMenu").show().addClass("expanded");
$(".menuExpansion .crime-type_accord").attr("src","/images/1_minus_icon2.png");
	}
else if (tidVal=="4") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
		$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
$(".menuExpansion  .criminal-justice-data-improvement-program_accord   + .expandMenu").show().addClass("expanded");
$(".menuExpansion .criminal-justice-data-improvement-program_accord").attr("src","/images/1_minus_icon2.png");
	}
	
else if (tidVal=="5") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
		$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
$(".menuExpansion .employment-and-expenditure_accord  + .expandMenu").show().addClass("expanded");
$(".menuExpansion .employment-and-expenditure_accord").attr("src","/images/1_minus_icon2.png");
	}	
	
else if (tidVal=="6") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
	$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
$(".menuExpansion .federal_accord  + .expandMenu").show().addClass("expanded");
$(".menuExpansion .federal_accord").attr("src","/images/1_minus_icon2.png");

	}
else if (tidVal=="7") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
	$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
	$(".menuExpansion .indian-country-justice-statistics_accord").attr("src","/images/1_minus_icon2.png");
	
$(".menuExpansion .indian-country-justice-statistics_accord + .expandMenu").show().addClass("expanded");
	}	
else if (tidVal=="8") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
	$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
	$(".menuExpansion .law-enforcement_accord").attr("src","/images/1_minus_icon2.png");

$(".menuExpansion .law-enforcement_accord + .expandMenu").show().addClass("expanded");
	}
else if (tidVal=="9") {
	$("#box3_content_inside .menuExpansion .expandMenu").hide();
		$("#box3_content_inside .menuExpansion").find("img").attr("src","/images/1_plus_icon2.png");
		$(".menuExpansion .victims_accord").attr("src","/images/1_minus_icon2.png");

$(".menuExpansion .victims_accord + .expandMenu").show().addClass("expanded");
	} 	
		});

</script>
